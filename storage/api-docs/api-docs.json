        "/api/games": {
            "post": {
                "tags": [
                    "Games"
                ],
                "summary": "Cria uma nova partida",
                "description": "Cria uma nova partida associada ao usuário autenticado",
                "operationId": "b8b9557618eb0f7b3650bdd4ab3a6487",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "type",
                                    "data_time",
                                    "club_id",
                                    "court_id",
                                    "status",
                                    "game_type"
                                ],
                                "properties": {
                                    "title": {
                                        "type": "string",
                                        "example": "Partida de Padel"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Partida amistosa"
                                    },
                                    "type": {
                                        "type": "string",
                                        "enum": [
                                            "public",
                                            "private"
                                        ],
                                        "example": "public"
                                    },
                                    "data_time": {
                                        "type": "string",
                                        "format": "date-time",
                                        "example": "2026-02-10 19:00:00"
                                    },
                                    "club_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "court_id": {
                                        "type": "integer",
                                        "example": 3
                                    },
                                    "custom_location": {
                                        "type": "string",
                                        "example": "Quadra externa"
                                    },
                                    "min_level": {
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "max_level": {
                                        "type": "integer",
                                        "example": 4
                                    },
                                    "max_players": {
                                        "type": "integer",
                                        "example": 4
                                    },
                                    "status": {
                                        "type": "string",
                                        "enum": [
                                            "open",
                                            "full",
                                            "in_progress",
                                            "completed",
                                            "canceled"
                                        ],
                                        "example": "open"
                                    },
                                    "price": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 120
                                    },
                                    "cost_per_player": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 30
                                    },
                                    "game_type": {
                                        "type": "string",
                                        "enum": [
                                            "casual",
                                            "competitive",
                                            "training"
                                        ],
                                        "example": "casual"
                                    },
                                    "duration_minutes": {
                                        "type": "integer",
                                        "example": 90
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Partida criada com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Erro de validação"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
