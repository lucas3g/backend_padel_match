                                    "full_name",
                                    "level",
                                    "side"
                                ],
                                "properties": {
                                    "full_name": {
                                        "type": "string",
                                        "maxLength": 255,
                                        "example": "João da Silva"
                                    },
                                    "phone": {
                                        "type": "string",
                                        "maxLength": 20,
                                        "example": "(49) 99999-9999"
                                    },
                                    "level": {
                                        "type": "integer",
                                        "example": 3
                                    },
                                    "side": {
                                        "type": "string",
                                        "enum": [
                                            "left",
                                            "right",
                                            "both"
                                        ],
                                        "example": "right"
                                    },
                                    "bio": {
                                        "type": "string",
                                        "example": "Jogador iniciante"
                                    },
                                    "profile_image_url": {
                                        "type": "string",
                                        "example": "https://site.com/foto.jpg"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Player criado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    },
