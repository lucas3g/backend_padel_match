                "summary": "Cria uma nova quadra",
                "description": "Cria uma quadra vinculada a um clube",
                "operationId": "d3f17259f42a90e559700103d00c99c1",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "club_id",
                                    "name",
                                    "description",
                                    "type"
                                ],
                                "properties": {
                                    "club_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "name": {
                                        "type": "string",
                                        "example": "Quadra Central"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Quadra coberta com iluminação"
                                    },
                                    "type": {
                                        "type": "string",
                                        "enum": [
                                            "padel",
