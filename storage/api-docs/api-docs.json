                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "type",
                                    "data_time",
                                    "club_id",
                                    "court_id",
                                    "status",
                                    "game_type"
                                ],
                                "properties": {
                                    "title": {
                                        "type": "string",
                                        "example": "Partida de Padel"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Partida amistosa"
                                    },
                                    "type": {
                                        "type": "string",
                                        "enum": [
                                            "public",
                                            "private"
                                        ],
                                        "example": "public"
                                    },
                                    "data_time": {
                                        "type": "string",
                                        "format": "date-time",
                                        "example": "2026-02-10 19:00:00"
                                    },
                                    "club_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "court_id": {
                                        "type": "integer",
                                        "example": 3
                                    },
                                    "custom_location": {
                                        "type": "string",
                                        "example": "Quadra externa"
                                    },
                                    "min_level": {
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "max_level": {
                                        "type": "integer",
                                        "example": 4
                                    },
                                    "max_players": {
                                        "type": "integer",
                                        "example": 4
                                    },
                                    "status": {
                                        "type": "string",
                                        "enum": [
                                            "open",
                                            "full",
                                            "in_progress",
                                            "completed",
                                            "canceled"
                                        ],
                                        "example": "open"
                                    },
                                    "price": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 120
                                    },
                                    "cost_per_player": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 30
                                    },
                                    "game_type": {
                                        "type": "string",
                                        "enum": [
                                            "casual",
                                            "competitive",
                                            "training"
                                        ],
                                        "example": "casual"
                                    },
                                    "duration_minutes": {
                                        "type": "integer",
                                        "example": 90
                                    }
                                },
                                "type": "object"
                            }
                        }
